{
    "name": "codedor/filament-menu",
    "description": "A menu package for filament",
    "keywords": [
        "codedor",
        "laravel",
        "filament-menu"
    ],
    "homepage": "https://github.com/codedor/filament-menu",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.12",
        "codedor/filament-link-picker": "dev-master",
        "codedor/filament-translatable-tabs": "dev-master"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\FilamentMenu\\": "src",
            "Codedor\\FilamentMenu\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\FilamentMenu\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\FilamentMenu\\Providers\\FilamentMenuServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
